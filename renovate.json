{
  "extends": [
    "config:base",
    ":automergeAll"
  ],
  "packageRules": [
    {
      "paths": ["docker-compose.master.yml"],
      "packagePatterns": ["^libero"],
      "versioning": "regex:^(?<compatibility>\\w+)-(?<hash>\\w+)$"
    },
    {
      "paths": ["docker-compose.infra.yml"],
      "packagePatterns": ["^minio"],
      "versioning": "regex:^(?<compatibility>\\w+)"
    }
  ]
}
