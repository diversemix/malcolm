{
  "extends": [
    "config:base",
    ":automergeAll"
  ],
  "packageRules": [
    {
      "managers": ["dockerfile", "docker-compose"],
      "packagePatterns": ["^libero"],
      "versioning": "regex:^master-(?<patch>\\w+)$"
    },
    {
      "managers": ["dockerfile", "docker-compose"],
      "packagePatterns": ["^minio"],
      "versioning": "regex:^RELEASE\\.(?<patch>\\.+)$"
    }
  ]
}
